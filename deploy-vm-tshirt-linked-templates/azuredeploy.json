{ "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",    "contentVersion": "1.0.0.0",  "parameters": {    "Server Prefix": {      "type": "string",      "defaultValue": "AZUEBT",      "maxLength": 7,      "metadata": {        "description": "Specify your server name acronym upto 7 characters. The naming convention of your server will be AZUEBTAPP01 where APP is your server acronym."      }    },    "Application Acronym": {      "type": "string",      "defaultValue": "DEMO",      "maxLength": 5,      "metadata": {        "description": "Specify your server name acronym upto 7 characters. The naming convention of your server will be AZUEBTAPP01 where APP is your server acronym."      }    },    "adminUsername": {      "type": "string",      "defaultValue": "azureadmin",      "metadata": {        "description": "Administrator user name used when provisioning virtual machines"      }    },    "adminPassword": {      "type": "securestring",      "minLength": 12,      "metadata": {        "description": "Administrator password used when provisioning virtual machines"      }    },    "tshirtSize": {      "type": "string",      "defaultValue": "Medium",      "allowedValues": [        "Small",        "Medium",        "Large",        "XLarge",        "XXLarge"      ],      "metadata": {        "description": "T-shirt size of the Nagios deployment"      }    },    "VM Template": {      "type": "string",      "defaultValue": "member-resources.json",      "metadata": {        "description": "Please select one of the available Virtual Machine sizes. This size will be provisioned for all Virtual Machines in this deployment."      }    },    "Number of VMs to Deploy": {      "type": "int",      "defaultValue": 2,      "allowedValues": [        1,        2,        3,        4,        5,        6,        7,        8,        9      ],      "metadata": {        "description": "Please select one of the available Virtual Machine sizes. This size 