yment."      }    },    "Select DataDisk Size": {      "type": "string",      "defaultValue": "1023",      "allowedValues": [        "128",        "256",        "512",        "1023"      ],      "metadata": {        "description": "Please select one of the available Virtual Machine sizes. This size will be provisioned for all Virtual Machines in this deployment."      }    },    "Extension Template": {      "type": "string",      "defaultValue": "extension-resources.json",      "metadata": {        "description": "Please select one of the available Virtual Machine sizes. This size will be provisioned for all Virtual Machines in this deployment."      }    },    "Enable Diagnostics": {      "type": "string",      "defaultValue": "true",      "allowedValues": [        "true",        "false"      ],      "metadata": {        "description": "Please select one of the available Virtual Machine sizes. This size will be provisioned for all Virtual Machines in this deployment."      }    }  },  "variables": {    "imagePublisher": "MicrosoftWindowsServer",    "imageOffer": "WindowsServer",    "windowsOSVersion": "2012-R2-Datacenter",    "compute": {      "virtualMachines": { "extensions": "2015-06-15" }    },    "clusterSpec": "[variables(concat('tshirtSize', parameters('tshirtSize')))]",    "vmName": "[toUpper(concat(parameters('Server Prefix'), parameters('Application Acronym')))]",    "vhdStorageAccountName": "[toLower(concat(variables('vmName'), 'osdisk'))]",    "dataDiskStorageAccountName": "[toLower(concat(variables('vmName'), 'datadisk'))]",    "diagStorageAccountName": "[toLower(concat(variables('vmName'), 'diag'))]",    "vnetRG": "VNET-RG",    "existingVNETName": "VNET",    "vnetID": "[resourceId(variables('vnetRG'), 'Microsoft.Network/virtualNetworks', variables('existingVNETName'))]",    "subnetRef": "[concat(variables('vnetID'),'/subnets/', variables('existingSubnetName'))]",    "existingSubnetName": "Mid_Tier_1",    "scriptUrl": "https://mylabtemplates.blob.core.